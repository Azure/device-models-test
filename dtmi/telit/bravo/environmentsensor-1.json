{
  "@id": "dtmi:telit:bravo:EnvironmentSensor;1",
  "@type": "Interface",
  "displayName": "BravoME910SensorInterfaces",
  "description": "Provides functionality to report the bravo Device environment Sensor data",
  "@context": "dtmi:dtdl:context;2",
  "contents": [
    {
      "@type": [
        "Telemetry",
        "Pressure"
      ],
      "displayName": "Environment_Pressure",
      "name": "pressure",
      "schema": "double",
      "unit": "pascal",
      "description": "Received Environment-pressure value (in double)"
    },
    {
      "@type": [
        "Telemetry",
        "Temperature"
      ],
      "schema": "double",
      "unit": "degreeCelsius",
      "displayName": "Environment_Temperature",
      "name": "temperature",
      "description": "Received Environment-temperature value (in double)"
    },
    {
      "@type": [
        "Telemetry",
        "Temperature"
      ],
      "schema": "double",
      "unit": "degreeCelsius",
      "displayName": "Environment_Rawtemperature",
      "name": "rawtemperature",
      "description": "Received Environment-rawtemperature value (in double)"
    },
    {
      "@type": [
        "Telemetry",
        "RelativeHumidity"
      ],
      "schema": "double",
      "unit": "unity percent",
      "displayName": "Environment_Humidity",
      "name": "humidity",
      "description": "Received Environment Relative humidity value (in double)"
    },
    {
      "@type": [
        "Telemetry",
        "Resistance"
      ],
      "schema": "double",
      "unit": "ohm",
      "displayName": "Environment_IndexForAirQualityvalue",
      "name": "IAQ_signal",
      "description": "Received Environment Index For Air Quality (IAQ) value(in double)"
    }
  ]
}